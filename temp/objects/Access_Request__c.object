    <validationRules>
        <fullName>SOE_Cannot_be_changed_after_created</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(SOE__c  )</errorConditionFormula>
        <errorMessage>SOE Cannot be changed after Acess Request is Created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_Rule_on_Domain_ID</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Domain__c,&apos;Other&apos;),ISBLANK(Domain_ID__c))</errorConditionFormula>
        <errorDisplayField>Domain_ID__c</errorDisplayField>
        <errorMessage>Domain ID field Should Not Be Empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_End_Date_1</fullName>
        <active>true</active>
        <errorConditionFormula>End_Time__c &lt;=  Start_Time__c</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>End Time must be greater than Start Time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Start_Date_1</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Time__c &lt;  NOW()</errorConditionFormula>
        <errorDisplayField>Start_Time__c</errorDisplayField>
        <errorMessage>Start Time should not be in the Past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Start_Date_2</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Time__c &lt;  SOE__r.Baseline_Start__c</errorConditionFormula>
        <errorMessage>Start Time should be greater than selected SOE Baseline Start date.</errorMessage>
    </validationRules>
