<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Member_Phone_Filed</fullName>
    <active>true</active>
    <errorConditionFormula>AND( OR(ISNULL( Salesforce_User__c ) ,ISBLANK(Salesforce_User__c )),OR(ISBLANK(Phone__c ), ISNULL( Phone__c )) )</errorConditionFormula>
    <errorDisplayField>Phone__c</errorDisplayField>
    <errorMessage>Please Enter Phone Number</errorMessage>
</ValidationRule>
