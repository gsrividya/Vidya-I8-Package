    <validationRules>
        <fullName>Shift_Type_is_On_Call</fullName>
        <active>true</active>
        <description>If shift type is &apos;On Call&apos; no.of hours cannot exceed 24.</description>
        <errorConditionFormula>AND( ISPICKVAL( Shift_Type__c , &apos;On Call&apos;) ,  (  End_Date__c  -  Start_Date__c  ) * 24   &gt;  24)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date cannot exceed 24 Hours from Start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Shift_Type_is_On_Site</fullName>
        <active>true</active>
        <description>If shift type is &apos;On Site&apos; no.of hours cannot exceed 24.</description>
        <errorConditionFormula>AND( ISPICKVAL( Shift_Type__c , &apos;On Site&apos;) ,  (  End_Date__c  -  Start_Date__c  ) * 24   &gt;  13)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date cannot exceed 13 Hours from Start date.</errorMessage>
    </validationRules>
