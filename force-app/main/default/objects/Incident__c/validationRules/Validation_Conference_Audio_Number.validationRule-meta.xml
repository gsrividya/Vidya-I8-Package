<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_Conference_Audio_Number</fullName>
    <active>true</active>
    <errorConditionFormula>NOT( OR( LEN ( Conference_Audio_Number__c ) = 0, REGEX(Conference_Audio_Number__c, &quot;[0-9 #*+-]+&quot;) ) )</errorConditionFormula>
    <errorMessage>Conference Audio Number accept only Digits and # * + -</errorMessage>
</ValidationRule>
