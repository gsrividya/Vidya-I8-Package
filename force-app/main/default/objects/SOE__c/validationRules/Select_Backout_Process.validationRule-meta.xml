<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Select_Backout_Process</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
      BackOut_Stage__c == true,ISBLANK(TEXT( Backout__c))
   )</errorConditionFormula>
    <errorDisplayField>Backout__c</errorDisplayField>
    <errorMessage>If you have Backout stage, then please select the Backout Process</errorMessage>
</ValidationRule>
