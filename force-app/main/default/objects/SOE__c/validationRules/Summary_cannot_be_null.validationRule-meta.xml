<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Summary_cannot_be_null</fullName>
    <active>true</active>
    <errorConditionFormula>OR( ISNULL( Summary__c ),  ISBLANK(Summary__c ) )</errorConditionFormula>
    <errorDisplayField>Summary__c</errorDisplayField>
    <errorMessage>Please enter summary about SOE</errorMessage>
</ValidationRule>
