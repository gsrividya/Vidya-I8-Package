<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_date_should_be_less_than_End_date</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Message_Start_Date__c  = Message_End_Date__c,Message_Start_Date__c  &gt; Message_End_Date__c)</errorConditionFormula>
    <errorMessage>End date  should be greater than Start date</errorMessage>
</ValidationRule>
