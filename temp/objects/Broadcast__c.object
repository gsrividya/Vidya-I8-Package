    <validationRules>
        <fullName>Message_Diplaying_Conditions</fullName>
        <active>true</active>
        <description>Enter the value in either All Users Check box or enter SOE.</description>
        <errorConditionFormula>OR(AND(All_Users__c == false,This_SOE_Users__c == false),
AND(All_Users__c == true,This_SOE_Users__c  == true)
)</errorConditionFormula>
        <errorMessage>Please select either  All Users  or This SOE Users</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Message_End_date_should_not_be_empty</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Message_End_Date__c)</errorConditionFormula>
        <errorMessage>Please enter End date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Message_Start_Date_Should_not_be_Empty</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Message_Start_Date__c )</errorConditionFormula>
        <errorMessage>Please enter Start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Message_should_not_be_empty</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Message__c)</errorConditionFormula>
        <errorMessage>Please enter Message</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_SOE</fullName>
        <active>true</active>
        <description>Please select SOE .</description>
        <errorConditionFormula>AND( This_SOE_Users__c == True , All_Users__c == False, ISBLANK( SOE__c ) )</errorConditionFormula>
        <errorDisplayField>SOE__c</errorDisplayField>
        <errorMessage>If we select This SOE user. SOE is Requried</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_date_must_be_Todays_date</fullName>
        <active>true</active>
        <errorConditionFormula>Message_Start_Date__c  &lt;  NOW()-0.0006945</errorConditionFormula>
        <errorMessage>Start date  must be  greater than or equal to Current time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_date_should_be_less_than_End_date</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Message_Start_Date__c  = Message_End_Date__c,Message_Start_Date__c  &gt; Message_End_Date__c)</errorConditionFormula>
        <errorMessage>End date  should be greater than Start date</errorMessage>
    </validationRules>
