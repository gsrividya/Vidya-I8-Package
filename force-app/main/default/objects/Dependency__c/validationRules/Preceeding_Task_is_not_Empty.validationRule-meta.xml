<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Preceeding_Task_is_not_Empty</fullName>
    <active>true</active>
    <errorConditionFormula>OR(ISNULL( Preceeding_Task__c ), ISBLANK(Preceeding_Task__c ) )</errorConditionFormula>
    <errorMessage>Preceeding Task cannot be empty.</errorMessage>
</ValidationRule>
